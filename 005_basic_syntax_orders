N = int(input())
total_price = 0

for order in range(N):
    price_per_capsule = float(input())
    days = int(input())
    capsules_per_day = int(input())
    if not (0.01 <= price_per_capsule <= 100.00 and 1 <= days <= 31 and 1 <= capsules_per_day <= 2000):
        continue
    else:
        price_of_order = price_per_capsule * capsules_per_day * days
        total_price += price_of_order
        print(f"The price for the coffee is: ${price_of_order:.2f}")

print(f"Total: ${total_price:.2f}")
