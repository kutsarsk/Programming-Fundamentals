def factorial_division(x, y):
    factorial_x = 1
    factorial_y = 1
    for n in range(1, x + 1):
        factorial_x *= n
    for m in range(1, y + 1):
        factorial_y *= m
    return factorial_x / factorial_y


number_1 = int(input())
number_2 = int(input())

print(f"{factorial_division(number_1, number_2):.2f}")
