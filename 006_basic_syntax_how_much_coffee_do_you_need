command = input()
coffees_count = 0

while command != "END":
    if command == "coding" or command == "dog" or command == "cat" or command == "movie":
        coffees_count += 1
    elif command == "CODING" or command == "DOG" or command == "CAT" or command == "MOVIE":
        coffees_count += 2
    else:
        command = input()
        continue
    command = input()

if coffees_count > 5:
    print("You need extra sleep")
else:
    print(f"{coffees_count}")
